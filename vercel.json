{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "framework": "create-react-app",
  "functions": {
    "server/dist/index.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/dist/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}